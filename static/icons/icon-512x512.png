    </defs>
    
    
    <!-- Background -->
    <rect width="512" height="512" fill="#4CAF50" rx="8"/>
    
    <!-- Grid patterns -->
    <rect width="512" height="512" fill="url(#bold-grid)"/>
    <rect width="512" height="512" fill="url(#grid)"/>
    
